{
  "schemaVersion": 1,
  "id": "discman_cli",
  "version": "${version}",

  "name": "MCS-Disman Client",
  "description": "A accompanying mod for servers managed by MCS-Discman to provide more data.",
  "authors": [
    "TreSet"
  ],
  "contact": {
    "homepage": "https://github.com/Tre5et/mcs-discman-client/releases",
    "sources": "https://github.com/Tre5et/mcs-discman-client"
  },

  "license": "GPL-V3",
  "icon": "assets/discman_cli/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.treset.discman_cli.DiscmanClientMod"
    ]
  },
  "mixins": [
    "discman_cli.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "~1.19",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
